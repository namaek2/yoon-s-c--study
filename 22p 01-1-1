#include <iostream>

int main()
{
    int arr[5] = {};

    std::cout << "1번째 정수 입력: ";
    std::cin >> arr[0];
    std::cout << "2번째 정수 입력: ";
    std::cin >> arr[1];
    std::cout << "3번째 정수 입력: ";
    std::cin >> arr[2];
    std::cout << "4번째 정수 입력: ";
    std::cin >> arr[3];
    std::cout << "5번째 정수 입력: ";
    std::cin >> arr[4];
    std::cout << "합게: " << arr[0] + arr[1] + arr[2] + arr[3] + arr[4];
}
